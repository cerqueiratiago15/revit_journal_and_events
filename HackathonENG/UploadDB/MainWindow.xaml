<Window x:Class="UploadDB.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UploadDB"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        
        <ProgressBar Width="600" Height="50" Maximum="100" Grid.Row="1" ></ProgressBar>
        <Button Width="100" Height="40" HorizontalAlignment="Left" Margin="10" FontSize="20">Folder</Button>
        <TextBox Width="650" Height="40" HorizontalAlignment="Right" Margin="20,0"></TextBox>
        
    </Grid>
</Window>
